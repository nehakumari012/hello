#include<iostream>
#include<stack>
using namespace std;
class Stack
{
    public:
    int *arr;
    int size;
    int top;
    Stack(int size)
    {
        this->size=size;
        arr=new int[size];
        top=-1;
    }
    void push(int element)
    {
        if(size-top>1)
        {
            top++;
            arr[top]=element;
        }
        else
        {
            cout<<"stack overflow"<<endl;
        }
    } 
    void pop()
    {
        if(top>=0)
        {
            top--;
        }
        else
        {
        cout<<"stack under flow"<<endl;
        }
    }
int peek()
{
    if(top>=0)
    {
        return arr[top];
    }
    else
    {
        cout<<"stack is empty"<<endl;
        return -1;
    }
}
bool isEmpty()
{
    if(top==-1)
    {
        return true;
    }
    return false;
}
};
int main()
{
    Stack st(5);
    st.push(9);
    st.push(89);
    st.push(45);
    st.push(34);
    cout<<st.peek()<<endl;
    st.pop();
    cout<<st.peek()<<endl;
    st.pop();
    cout<<st.peek()<<endl;
    st.pop();
    cout<<st.peek()<<endl;
    if(st.isEmpty())
    {
        cout<<"stack is empty"<<endl;
    }
    else
    {
        cout<<"stack is not empty"<<endl;
    }
return 0;
}
